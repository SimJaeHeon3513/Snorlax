#include <stdio.h>
#define MAX 15
char arr[5][MAX];
int main(){
    
    for(int i = 0; i < 5; i++){
        scanf("%s", arr[i]);
    }
    
    for(int j = 0; j < MAX; j++){
        for(int i = 0; i < 5; i++){
            if(arr[i][j] == NULL){
                continue;
            }
            else{
                printf("%c", arr[i][j]);
            }
        }
    }
    printf("\n");
    
    return 0;
}
